#include <stdio.h>

int main() {
    int X, Y;
    scanf("%d %d", &X, &Y);

    double precos[] = {4.00, 4.50, 5.00, 2.00, 1.50};

    if (X >= 1 && X <= 5) {

        double total = Y * precos[X - 1];

        printf("Total: R$ %.2lf\n", total);
    } else {

        printf("Código inválido\n");
    }

    return 0;
}
